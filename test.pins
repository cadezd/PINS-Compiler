var data: arr[10] integer;

fun find(elt: integer, low: integer, high: integer, tab: arr[10] integer) : integer = (
    {middle = low + ((high - low) / 2)},
    {if elt == tab[middle] then (
        {rez = middle}
    ) else { if elt < tab[middle] then (
        {rez = find(elt, low, middle - 1, tab)}
    ) else (
         {rez = find(elt, middle + 1, high, tab)}
    )}},
    rez
) {where var middle: integer; var rez: integer};

